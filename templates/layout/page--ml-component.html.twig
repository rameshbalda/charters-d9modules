<div class="page">
  <a name="Top" id="Top"></a>
  {% include directory ~ '/templates/layout/header.html.twig' %}
    <div id="main" class="content-area model-law-component" role="main">
    <a id="main-content"></a>
    {% include directory ~ '/templates/layout/lead-image.html.twig' %}
    <div class="main-container container">
      <div class="row">
        <div class="col-sm-12">
          {{ page.highlighted }}
          {% if action_links %}
            <nav class="action-links">{{ action_links }}</nav>
          {% endif %}
          {{ page.content_top }}
        </div>
      </div>
    </div>
    <div class="container">
      <div class="row">
        <div class="col-sm-12">
          {{ page.content }}
        </div>
      </div>
      <div class="row">
        <div class="col-sm-12 model-lower">
          <h3 class="section-title">State Scores for this Component</h3>
          {{ page.content_lower }}
        </div>
      </div>
		</div>
    <a href="#Top" class="btn-btt" title="Back to Top" style="display: block;"></a>          
    {% include directory ~ '/templates/layout/footer.html.twig' %}
  </div>
</div>
